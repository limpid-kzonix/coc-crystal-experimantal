{
  "name": "coc-crystal-experimental",
  "version": "0.0.1",
  "description": "Crystal language server extension for coc.nvim",
  "main": "lib/index.js",
  "publisher": "limpid-kzonix",
  "keywords": [
    "coc.nvim",
    "crystal"
  ],
  "engines": {
    "coc": "^0.0.78"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limpid-kzonix/coc-crystal-experimental.git"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "webpack",
    "prepare": "npx npm-run-all clean build"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "Crystal configuration object",
      "properties": {
        "crystal.server": {
          "type": "string",
          "description": "Absolute path to 'scry' executable binary."
        },
        "crystal.completion": {
          "type": "boolean",
          "default": true
        },
        "crystal.hover": {
          "type": "boolean",
          "default": true
        },
        "crystal.implementations": {
          "type": "boolean",
          "default": true
        },
        "crystal.logLevel": {
          "type": "string",
          "default": "debug"
        },
        "crystal.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable/Disable extension."
        }
      }
    }
  },
  "author": "balyszyn@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@chemzqm/tsconfig": "0.0.3",
    "@chemzqm/tslint-config": "1.1.0",
    "@types/colors": "1.2.1",
    "@types/debounce": "1.2.0",
    "@types/node": "13.13.42",
    "@types/uuid": "8.3.0",
    "@types/which": "2.0.0",
    "coc.nvim": "0.0.80",
    "colors": "1.4.0",
    "debounce": "1.2.0",
    "iconv-lite": "0.6.2",
    "request-light": "0.4.0",
    "rimraf": "3.0.2",
    "timeago.js": "4.0.2",
    "ts-loader": "8.0.17",
    "tslint": "6.1.3",
    "typescript": "4.1.5",
    "uuid": "8.3.2",
    "vscode-languageserver-types": "3.16.0",
    "webpack": "5.23.0",
    "webpack-cli": "4.5.0",
    "which": "2.0.2"
  },
  "dependencies": {}
}
